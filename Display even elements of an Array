#include <stdio.h>
#include<conio.h>
int main()
{
    int i, n, search, A[100], FLAG=0;
    
    printf("Enter number of elements\n");
    scanf("%d",&n);
    
    printf("Enter %d elements\n", n);
    
    for (i = 0; i < n; i++)
    {
        scanf("%d",&A[i]);
    }
    
    printf("Enter value to find\n");
    scanf("%d", &search);
    
   for (i = 0; i < n; i++)
    {
        if(A[i]%2==0)
        {
        printf("%d",A[i]);
        FLAG=1;
        }
    }
    
    if(FLAG==0)
    {
    printf("No even no. present in the array")
    }
    
    return 0;   
}
